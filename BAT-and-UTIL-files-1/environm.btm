        rem Ways to keep this script from being run too often:
                rem 1) TCSTART.BAT can pass a "TCSTART" parameter so that we can tell when this has been run and not run it repeatedly
                rem 2) Secondary shells start from a different INI-specified location, using a different TCStart.bat, which sets SECONDARYSHELL=1
                rem 3) We also set ENVIRONM_RUN=1 after running, to prevent successive timewasting runs
        iff "%1" eq "force" .or. "%1" eq "-force" .or. "%1" eq "validate" then
                rem We are in force mode! Skip these particular validations!
        else                
            if  "1" eq "%ENVIRONM_RUN%"                                  (goto :The_Very_END  )   %+ REM added this line 2023/07/26
            if  "1" eq "%ENVIRONM_RUN%" .and. "%@UPPER[%1]" eq "TCSTART" (goto :The_Very_END  )   %+ REM so as of 2023/07/26 this line should never happen
            if  "1" eq "%SECONDARYSHELL%"                                (goto :SecondaryShell)
        endiff

    rem DEBUG & VALIDATIONS can be turned on with these:
        if "%@UPPER[%1]" eq "VALIDATE" (set PERFORM_VALIDATIONS=1 %+ set DEBUG_VALIDATIONS=1 %+ call wake-all-drives)
        if "%@UPPER[%1]" eq "AUTOEXEC" (set PERFORM_VALIDATIONS=1 %+ set AUTOEXEC=1                                 )
        if "%@UPPER[%1]" eq "DEBUG"    (set   DEBUG_VALIDATIONS=1 %+ set DEBUG_TMP=%DEBUG%   %+ set DEBUG=1         )

    rem VALIDATIONS take a full 0.5 seconds longer if you actually call the routine, vs turning the call itself into a REM statement when we're not doing them
                                        set gosubValidateFolderExists=rem gosub ValidateFolderExists
        if %PERFORM_VALIDATIONS eq 1   (set gosubValidateFolderExists=    gosub ValidateFolderExists)

    rem Similar approach to reduce time-footprint of the performance tuning itself:      
                                        set QUIET_TIMER_OR_NOT=/q
                                        set gosub_stopwatch_button=rem gosub stopwatch_button
        if %PERFORMANCE_TUNING  eq 1   (set gosub_stopwatch_button=    gosub stopwatch_button
                                        set QUIET_TIMER_OR_NOT=/9 /s)

