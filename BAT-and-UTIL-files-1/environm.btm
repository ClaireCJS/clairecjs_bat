        rem ‼ ‼ ‼ ‼ ‼ ‼ ‼ ‼ ‼ ‼ ‼ rainbow-cursor-color progress-indicator cosmetics ‼ ‼ ‼ ‼ ‼ ‼ ‼ ‼ ‼ ‼ ‼ 
        rem 🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈🌈

                rem Sample: change cursor color to red: echos %@CHAR[27][ q%@CHAR[27]]12;red%@CHAR[7] 
                
                :set_next_cursor_color_in_rainbow_sequence
                
                
                        rem Initialize the array and counter — we will use the fact that number_of_rgb_colorsteps is defined as the litmus test of needing to define it or not ... Unless we're in 
                        if defined number_of_rgb_colorsteps .and. "%1" ne "force" goto :RGB_Array_Has_Been_Set_Already
                        iff "%@ARRAYINFO[rgb_steps_hex]" ne "-1" then
                                set array_exists=1
                        else 
                                set array_exists=0
                        endiff                
                        rem echo array exists=%array_exists%
                        iff "%1" eq "force" .and. %array_exists eq 1 then
                                UnsetArray /q rgb_steps_hex
                        endiff                                
                        rem echo %reverse_on%set_next_cursor_color_in_rainbow_sequence called, rgb_index is %rgb_index%%reverse_off% array_exists=%array_exists%
rem                     if %array_exists eq 1 .or. "%1" ne "force" (goto :RGB_Array_Has_Been_Set_Already) ///NO!
                        if %array_exists eq 1                      (goto :RGB_Array_Has_Been_Set_Already)
                                set number_of_rgb_colorsteps=21
                                SetArray  rgb_steps_hex[%number_of_rgb_colorsteps%]
                                rem rray  rgb_steps_rgb[%number_of_rgb_colorsteps%]
                                set       rgb_steps_hex[00]=FF0000          %+ rem set rgb_steps_rgb[20]=255,0,255          %+ rem oops backwards
                                set       rgb_steps_hex[01]=FF3F00          %+ rem set rgb_steps_rgb[19]=191,0,255          %+ rem oops backwards
                                set       rgb_steps_hex[02]=FF7F00          %+ rem set rgb_steps_rgb[18]=127,0,255          %+ rem oops backwards
                                set       rgb_steps_hex[03]=FFBF00          %+ rem set rgb_steps_rgb[17]=63,0,255           %+ rem oops backwards
                                set       rgb_steps_hex[04]=FFFF00          %+ rem set rgb_steps_rgb[16]=0,0,255            %+ rem oops backwards
                                set       rgb_steps_hex[05]=BFFF00          %+ rem set rgb_steps_rgb[15]=0,63,255           %+ rem oops backwards
                                set       rgb_steps_hex[06]=7FFF00          %+ rem set rgb_steps_rgb[14]=0,127,255          %+ rem oops backwards
                                set       rgb_steps_hex[07]=3FFF00          %+ rem set rgb_steps_rgb[13]=0,191,255          %+ rem oops backwards
                                set       rgb_steps_hex[08]=00FF00          %+ rem set rgb_steps_rgb[12]=0,255,255          %+ rem oops backwards
                                set       rgb_steps_hex[09]=00FF3F          %+ rem set rgb_steps_rgb[11]=0,255,191          %+ rem oops backwards
                                set       rgb_steps_hex[10]=00FF7F          %+ rem set rgb_steps_rgb[10]=0,255,127          %+ rem oops backwards
                                set       rgb_steps_hex[11]=00FFBF          %+ rem set rgb_steps_rgb[09]=0,255,63           %+ rem oops backwards
                                set       rgb_steps_hex[12]=00FFFF          %+ rem set rgb_steps_rgb[08]=0,255,0            %+ rem oops backwards
                                set       rgb_steps_hex[13]=00BFFF          %+ rem set rgb_steps_rgb[07]=63,255,0           %+ rem oops backwards
                                set       rgb_steps_hex[14]=007FFF          %+ rem set rgb_steps_rgb[06]=127,255,0          %+ rem oops backwards
                                set       rgb_steps_hex[15]=003FFF          %+ rem set rgb_steps_rgb[05]=191,255,0          %+ rem oops backwards
                                set       rgb_steps_hex[16]=0000FF          %+ rem set rgb_steps_rgb[04]=255,255,0          %+ rem oops backwards
                                set       rgb_steps_hex[17]=3F00FF          %+ rem set rgb_steps_rgb[03]=255,191,0          %+ rem oops backwards
                                set       rgb_steps_hex[18]=7F00FF          %+ rem set rgb_steps_rgb[02]=255,127,0          %+ rem oops backwards
                                set       rgb_steps_hex[19]=BF00FF          %+ rem set rgb_steps_rgb[01]=255,63,0           %+ rem oops backwards
                                set       rgb_steps_hex[20]=FF00FF          %+ rem set rgb_steps_rgb[00]=255,0,0            %+ rem oops backwards
                                if "%1" ne "force" (set rgb_index=0)
                                rem set   DEBUG_CURSOR=2
                        :RGB_Array_Has_Been_Set_Already

                        iff %rgb_index% eq %number_of_rgb_colorsteps% then
                                set rgb_index=1
                        endiff                        
                        iff %rgb_index% LE %number_of_rgb_colorsteps% then
                                set color=%rgb_steps_hex[%rgb_index]
                                rem debug: set color_setter= @ANSI_CURSOR_COLOR_BY_HEX[%color] %@ANSI_CURSOR_COLOR_BY_HEX[%color]
                                rem color_setter=%@ANSI_CURSOR_COLOR_BY_HEX[%color]
                                set color_setter=%@char[27][ q%@CHAR[27]]12;#%[color]%@CHAR[7]
                                rem Get individual R,G,B values:
                                        set r=%@word[0,%color%,,] 
                                        set g=%@word[1,%color%,,] 
                                        set b=%@word[2,%color%,,]
                                iff %DEBUG_CURSOR% GT 1 then
                                        echo %color_setter%%@format[2,%rgb_index%] TEST - This: color=%@format[11,%color%] / r=%@format[3,%r], g=%@format[3,%g], b=%@format[3,%b] / %%@CHAR[27][38;2;%r%;%g%;%b%m%color%@CHAR[27][0m%%@CHAR[7] - 
                                        rem *pause>nul
                                        delay /m 250
                                else
                                        echos %color_setter%
                                        rem                                         *pause>nul
                                endiff
                                set /a rgb_index=%rgb_index%+1
                                return
                        else
                                rem echo All values processed.
                                return
                        endiff
                :return
                
